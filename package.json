{
  "name": "vite-project",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "homepage": "https://fengtomy.github.io/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "upgrade:major": "node ./scripts/update-version.js --version=major",
    "upgrade:minor": "node ./scripts/update-version.js --version=minor",
    "upgrade:patch": "node ./scripts/update-version.js --version=patch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "postbuild": "cp dist/index.html dist/404.html",
    "addBlog": "node scripts/add-blog-post.js",
    "addPostDate": "node scripts/add-publish-date.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "mdast-util-from-markdown": "^2.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.7.0",
    "react-syntax-highlighter": "^15.6.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.2.0",
    "jsdom": "^27.3.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.16"
  }
}
